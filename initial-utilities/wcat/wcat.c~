#include <stdio.h>

int main(int argc, char *argv[]){
	FILE *file;
	for(int i =1;i<argc;i++){
		file=fopen(argv[i],"r");
		if(file == NULL){
			printf("cannot open file: %s\n", argv[i]);
			return 0;
		}
		char ch;
		while((ch=fgetc(file))!=EOF){
			putchar(ch);
		}
		printf("\n");
		fclose(file);
	}
	return 0;
}
